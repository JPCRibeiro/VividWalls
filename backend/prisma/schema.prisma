generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Posts {
  id            Int      @id @default(autoincrement())
  imageName     String
  imageHash     String   @unique
  caption       String
  totalLikes    Int      @default(0)
  created       DateTime @default(now())
}